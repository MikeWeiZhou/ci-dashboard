{
  "name": "cidashboard",
  "description": "Continuous Integration Dashboard for ***REMOVED*** Inc.",
  "version": "1.0.0",
  "scripts": {
    "setup": "npm run setup-front && npm run setup-back",
    "setup-front": "cd react-demo && npm install && cd .. && npm run build-front",
    "setup-back": "npm install && npm run build-back && npm run setup-db",
    "setup-db": "ts-node source/setupdb",
    "reset-db": "ts-node source/dropdb && npm run setup-db",
    "-----------------------------------------------------------------------------/": "",
    "build": "npm run build-front && npm run build-back",
    "build-front": "cd react-demo && npm run build && cd ..",
    "build-back": "tsc",
    "----------------------------------------------------------------------------//": "",
    "start": "node build/start",
    "---------------------------------------------------------------------------///": "",
    "dev-back": "ts-node source/start",
    "dev-front": "npm run --prefix react-demo start",
    "--------------------------------------------------------------------------////": "",
    "test": "npm run build-back && mocha build/unittests/**/*.js",
    "bt": "npm run build-back && node build/test_random",
    "dt": "ts-node source/test_random"
  },
  "author": "Johnny Lee, Elisa Chu, Tony So, Mike Zhou",
  "homepage": "https://bitbucket.org/tsungyulee/kpi-dashboard#readme",
  "dependencies": {
    "JSONStream": "^1.3.2",
    "csv-parse": "^2.2.0",
    "express": "^4.16.3",
    "moment": "^2.22.1",
    "mysql": "^2.15.0",
    "python-shell": "^0.5.0"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.1.11",
    "@types/express": "^4.11.1",
    "@types/mocha": "^5.2.0",
    "@types/moment": "^2.13.0",
    "@types/mysql": "^2.15.4",
    "@types/node": "^10.0.3",
    "@types/python-shell": "^0.4.1",
    "mocha": "^5.1.1",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3"
  }
}
