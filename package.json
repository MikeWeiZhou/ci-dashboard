{
  "name": "cidashboard",
  "description": "Continuous Integration Dashboard for ***REMOVED*** Inc.",
  "version": "1.0.0",
  "scripts": {
    "setup": "npm install && cd react-demo && npm install && cd .. && npm run compile",
    "start": "npm run --prefix react-demo start",
    "/-----------------------------------------------------------------------------": "",
    "compile": "tsc -p source --outDir compiled",
    "//----------------------------------------------------------------------------": "",
    "test": "npm run compile && mocha compiled/unittests/tests.js && npm run testpipeline",
    "testpipeline": "node compiled/test_pipeline_in.js && node compiled/test_pipeline_out.js",
    "///---------------------------------------------------------------------------": "",
    "ct": "npm run compile && node compiled/test_random.js"
  },
  "author": "Johnny Lee, Elisa Chu, Tony So, Mike Zhou",
  "homepage": "https://bitbucket.org/tsungyulee/kpi-dashboard#readme",
  "dependencies": {
    "JSONStream": "^1.3.2",
    "csv-parse": "^2.2.0",
    "moment": "^2.22.1",
    "mysql": "^2.15.0",
    "python-shell": "^0.5.0"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.1.11",
    "@types/mocha": "^5.2.0",
    "@types/moment": "^2.13.0",
    "@types/mysql": "^2.15.4",
    "@types/node": "^10.0.3",
    "@types/python-shell": "^0.4.1",
    "mocha": "^5.1.1",
    "typescript": "^2.8.3"
  }
}
