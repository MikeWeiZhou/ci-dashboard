{
  "name": "cidashboard",
  "description": "Continuous Integration Dashboard for ***REMOVED*** Inc.",
  "version": "1.0.0",
  "scripts": {
    "setup": "npm install && npm run compile",
    "start": "node compiled/server.js",
    "compile": "tsc -p source --outDir compiled",
    "cnr": "npm run compile && npm start",
    "test": "mocha compiled/unittests/tests.js",
    "testpipeline": "node compiled/test_pipeline_json_to_db.js",
    "compiletests": "tsc -p source/unittests --outDir compiled/unittests",
    "cnrt": "npm run compiletests && npm test"
  },
  "author": "Johnny Lee, Elisa Chu, Tony So, Mike Zhou",
  "homepage": "https://bitbucket.org/tsungyulee/kpi-dashboard#readme",
  "dependencies": {
    "JSONStream": "^1.3.2",
    "csv-parse": "^2.2.0",
    "mysql": "^2.15.0"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.1.11",
    "@types/mocha": "^5.2.0",
    "@types/mysql": "^2.15.4",
    "@types/node": "^10.0.3",
    "mocha": "^5.1.1",
    "typescript": "^2.8.3"
  }
}
